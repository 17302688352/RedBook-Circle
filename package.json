{
  "name": "taro3-vue3-pinia",
  "version": "1.0.0",
  "private": true,
  "description": "taro3-vue3-pinia",
  "templateInfo": {
    "name": "default",
    "typescript": true,
    "css": "sass"
  },
  "scripts": {
    "build:weapp": "taro build --type weapp",
    "build:swan": "taro build --type swan",
    "build:alipay": "taro build --type alipay",
    "build:tt": "taro build --type tt",
    "build:h5": "taro build --type h5",
    "build:rn": "taro build --type rn",
    "build:qq": "taro build --type qq",
    "build:jd": "taro build --type jd",
    "build:quickapp": "taro build --type quickapp",
    "dev:weapp": "npm run build:weapp -- --watch",
    "devtools:weapp": "npm run build:weapp -- --watch --devtools",
    "dev:swan": "npm run build:swan -- --watch",
    "dev:alipay": "npm run build:alipay -- --watch",
    "dev:tt": "npm run build:tt -- --watch",
    "dev:h5": "npm run build:h5 -- --watch",
    "dev:rn": "npm run build:rn -- --watch",
    "dev:qq": "npm run build:qq -- --watch",
    "dev:jd": "npm run build:jd -- --watch",
    "dev:quickapp": "npm run build:quickapp -- --watch",
    "typecheck": "vue-tsc --noEmit",
    "lint": "eslint --fix ./ --ext .vue,.js,jsx,.ts,tsx",
    "prepare": "husky install",
    "updates": "yarn upgrade-interactive --latest"
  },
  "lint-staged": {
    "*.{vue,js,jsx,ts,tsx}": "eslint --fix"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-customizable"
    }
  },
  "dependencies": {
    "@babel/runtime": "^7.17.2",
    "@nutui/nutui-taro": "^3.1.17",
    "@tarojs/cli": "^3.4.3",
    "@tarojs/components": "^3.4.3",
    "@tarojs/plugin-html": "^3.4.3",
    "@tarojs/runtime": "^3.4.3",
    "@tarojs/taro": "^3.4.3",
    "pinia": "^2.0.11",
    "qs": "^6.10.3",
    "vue": "^3.2.31"
  },
  "devDependencies": {
    "@babel/core": "^7.17.5",
    "@commitlint/cli": "^16.2.1",
    "@commitlint/config-conventional": "^16.2.1",
    "@tarojs/mini-runner": "^3.4.3",
    "@tarojs/plugin-vue-devtools": "^3.4.3",
    "@tarojs/webpack-runner": "^3.4.3",
    "@types/qs": "^6.9.7",
    "@types/webpack-env": "^1.16.3",
    "@typescript-eslint/eslint-plugin": "^5.14.0",
    "@typescript-eslint/parser": "^5.14.0",
    "@vue/compiler-sfc": "^3.2.31",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/eslint-config-typescript": "^10.0.0",
    "babel-plugin-import": "^1.13.3",
    "babel-preset-taro": "^3.4.3",
    "commitizen": "^4.2.4",
    "cz-conventional-changelog": "^3.3.0",
    "cz-customizable": "^6.3.0",
    "eslint": "^8.10.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-taro": "^3.4.3",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-vue": "^8.5.0",
    "husky": "^7.0.4",
    "lint-staged": "^12.3.5",
    "prettier": "^2.5.1",
    "taro-plugin-pinia": "^1.0.0",
    "taro-plugin-tailwind": "^1.2.8",
    "typescript": "^4.6.2",
    "vue-loader": "^17.0.0",
    "vue-tsc": "^0.32.1"
  }
}
